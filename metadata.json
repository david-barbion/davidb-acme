{
  "name": "davidb-acme",
  "version": "0.1.0",
  "author": "David Barbion",
  "summary": "Puppet Let's encrypt ACME tiny module",
  "license": "MIT",
  "source": "https://github.com/david-barbion/davidb-acme.git",
  "project_page": "http://github.com/david-barbion/davidb-acme",
